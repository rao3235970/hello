/* 图片滚动 */
.box-sv {
  width: 700rpx;
  padding: 20rpx 0;
  height: 176rpx;
  white-space:nowrap;
  margin:3% auto;
  }
.box-sv image {
  width: 296rpx;
  height: 178rpx;
  margin-right: 20rpx;
  border-radius: 5px;
  }
  /* .videoSrc {
    width: 292rpx;
