/* pages/team/team.wxss */
.list{
    border-top: 1px solid #ECECEC;
    overflow: hidden;
}
.list .li{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    padding: 30rpx;
    border-bottom: 1px solid #ECECEC
}
.list .li .name{
    width: 500rpx;
    font-size: 35rpx;
    line-height: 50rpx;
}
.list .li .name text{
    display: block;
}
.list .li .name text:last-child{
    font-size: 30rpx;
    color: #0F1D37;
}
.list .li .arrow{
    width: 25rpx;
}
.list .tips{
    font-size: 30rpx;
    text-align: center;
    margin-top: 100rpx;
}