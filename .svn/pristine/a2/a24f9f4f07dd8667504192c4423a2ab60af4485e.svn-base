
.text-slh{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.item .head{
  min-width:82rpx;
  height: 82rpx;
  border-radius: 50%;
  margin-right: 19rpx;

}
.list-wrap{
  padding: 0 31rpx 0 29rpx;
}
.item{
  padding: 29rpx 0 40rpx;
  display: flex;
  border-bottom: 1rpx solid #eee;
}
.content{
  flex: 2;
}
.name-time{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.name-time text:nth-of-type(1){
  font-size:30rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(51,51,51,1);
}
.name-time text:nth-of-type(2){
  font-size:24rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(153,153,153,1);
}
.content-title{
  width:598rpx;
  font-size:30rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(15,29,55,1);
  margin-top: 15rpx;
}
.content-text{
  font-size:30rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(15,29,55,1);
  width:578rpx;
  min-height:60rpx;
  background:rgba(236,236,236,1);
  border-radius:8rpx;
  margin-top: 30rpx;
  line-height: 46rpx;
  text-indent: 32rpx;
  padding:20rpx 10rpx;

}
.content-video{
  width: 598rpx;
  height:250rpx;
  margin-top:30rpx;
  position: relative;
}
.content-video image:nth-of-type(1){
  width: 598rpx;
  border-radius: 4rpx;
  height:250rpx;
  

}
.content-video image:nth-of-type(2){
  width: 60rpx !important;
  border-radius: 0;
  height: 60rpx;
  position: absolute;
  top: 50%;
  margin-top: -30rpx;
  left: 50%;
  margin-left: -30rpx;
}
.requestion{
  width: 102rpx;
  height: 102rpx;
  position: fixed;
  right: 16rpx;
  bottom: 200rpx;
}
.requestion image{
  width: 100%;
  height: 100%;
  display: block;
}
.isNull{
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 200rpx;
  font-size: 50rpx;
color:rgba(153,153,153,1);
}