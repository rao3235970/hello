/* pages/study-tour/study-tour.wxss */

.container {
  /* background-color: #F7F7F7; */
  width: 100%;
  height: 100%;
  padding: 0;
}
/* 搜索框 */
.searcher {
  width:93%;
  height: 76rpx;
  margin-top: 20rpx;
  background-color: #f5f5f5;
}
.searcher image {
  width: 26rpx;
  height: 26rpx;
  float: left;
  margin-left:244rpx;
  margin-top: 24rpx;
}
.searcher input {
  width: 224rpx;
  height: 26rpx;
  float: left;
  margin-left: 10rpx;
  margin-top: 12rpx;
  font-size: 28rpx;
  color: #999;
}
/* 游学列表 */
.content {
  background-color: #F7F7F7;
  width: 100%;
}
.studyList {
  background-color: #ffffff;
  width: 100%;
  height: 288rpx;
  border-bottom: 20rpx solid #F7F7F7;
}
.studyListChild {
  margin-top: 20rpx;
}
.studyList image {
  width: 216rpx;
  height: 216rpx;
  display: block;
  float: left;
  margin-top: 36rpx;
  margin-left: 30rpx;
}
.studyText {
  width: 420rpx;
  float: left;
  margin-left: 52rpx;
  margin-top: 26rpx;
}
.textTitle {
  font-size: 32rpx;
  color: #454545;
}
.textContent {
  /* 做省略限制 */
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 28rpx;
  color: #9e9e9e;
  height: 78rpx;
  margin-top: 20rpx;
}
.textMoney {
  display: block;
  color: #ff135b;
  font-size: 32rpx;
  margin-top: 30rpx;
  font-weight: 700;
  float: left;
}
.textDiscuss {
  display: block;
  color: #9e9e9e;
  font-size: 28rpx;
  float: left;
  margin-left: 178rpx;
  margin-top: 38rpx;
}
