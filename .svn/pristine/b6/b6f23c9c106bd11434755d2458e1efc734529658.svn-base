<!--pages/study-plan/study-plan.wxml-->
<!-- <text>pages/study-plan/study-plan.wxml</text> -->
<view class="viewForm">
  <!-- 留学申请 -->
  <view class="formTitle">留学申请</view>
  <view class="common">
    <picker mode="date" value="{{date}}" name='create_time' start="2015-09-01" bindchange="bindDateChange">
      <view class="picker common">
        日期选择: {{date}}
      </view>
    </picker>
  </view>
  <view class="viewInput">
    <input type="text" placeholder='填写最高学历'></input>
  </view>
  <view class="viewInput">
    <input type="text" placeholder='请输入姓名'></input>
  </view>
  <view class="viewInput">
    <input type="text" placeholder='请输入微信'></input>
  </view>
  <view class="viewInput">
    <input type="text" placeholder='请输入电话'></input>
  </view>
  <view class="viewButton">获取留学方案</view>
</view>