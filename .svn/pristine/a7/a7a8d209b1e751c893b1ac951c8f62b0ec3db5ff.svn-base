/* pages/information-details/information-details.wxss */
.contain {
  padding: 68rpx 30rpx;
}

.viewTitle {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 50rpx;
  font-weight: 700;
  color: #454545;
  width: 674rpx;
  height: 148rpx;
  line-height: 78rpx;
}

.viewDate {
  margin-top: 36rpx;
  font-size: 28rpx;
  color: #9e9e9e;
}

.viewContent {
  margin-top: 66rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 5;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 36rpx;
  color: #454545;
  width: 100%;
  height: 260rpx;
}
.imageContent {
  display: block;
  width: 100%;
  height: 380rpx;
  margin-top: 60rpx;
}
