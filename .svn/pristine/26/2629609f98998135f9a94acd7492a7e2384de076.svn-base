@import "../../wxParse/wxParse.wxss";
/* 轮播图 */
swiper {
  height: 470rpx;
}
.swiper image{
  height: 470rpx;
}
/* 基本信息 */
.basicInform {
  padding: 40rpx 30rpx;
  border-bottom: 20rpx solid #F6F6F6;
  }
/* 景区名 */
.scenicName {
  font-size: 40rpx;
  color: #333333;
  font-weight: 900;
}
/* 旅游时间 */
.scenicDate {
  font-size: 28rpx;
  color: #333333;
  margin-top: 20rpx;
}
/* input和价格 */
.viewMoney {
  margin-top: 42rpx;
  overflow: hidden;
}
/* 旅游价格 */
.scenicMoney {
  font-size: 40rpx;
  font-weight: 700;
  color: #FF135B;
  float: left;
}
/* 加减input框 */
/*主容器*/
.stepper {
  width:176rpx;
  height:52rpx;
  margin: 0 auto;
  border: 1px solid #ccc;
  border-radius: 6rpx;
  float: left;
  margin-left: 318rpx;
}
/*加号和减号*/
.stepper text {
  float: left;
  width: 38rpx;
  line-height: 52rpx;
  text-align: center;
  color: #333333;
  margin-left: 6rpx;
}
/*数值*/
.stepper input {
  width: 80rpx;
  height: 52rpx;
  float: left;
  margin: 0 auto;
  text-align: center;
  font-size: 32rpx;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
  color: #333333;
}
/*普通样式*/
.stepper .normal {
  color:black;
}
/*禁用样式*/
.stepper .disable {
  color: #ccc;
}
/* 规定 */
.viewruler {
  margin-top: 10rpx;
  overflow: hidden;
}
/* 12月可定 */
.scenicReserve {
  font-size: 28rpx;
  color: #999999;
  float: left;
}
/* 往返自理 */
.scenicSelf {
  font-size: 28rpx;
  color: #999999;
  float: left;
  margin-left: 40rpx;
}
/* 评论和查看更多*/
.viewDiscuss {
  padding: 30rpx;
  border-bottom: 20rpx solid #F6F6F6;
  overflow: hidden;
  }
/* 评论 */
.discussText {
  font-size: 28rpx;
  color: #333333;
  float: left;
}
/* 查看更多 */
.discussMore {
  font-size: 28rpx;
  color: #999999;
  float: left;
  margin-left: 430rpx;
}
/* 小箭头 */
.imageMore {
  width: 26rpx;
  height: 26rpx;
  display: block;
  float: left;
  margin-left: 3.6rpx;
  margin-top: 6rpx;
}
