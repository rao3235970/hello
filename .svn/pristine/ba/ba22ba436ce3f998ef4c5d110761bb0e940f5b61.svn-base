/* pages/study-tour/study-tour.wxss */
/* 搜索框 */
text view image {
  margin: 0rpx;
  padding: 0rpx;
}
.searcher {
  width: 100%;
  height: 76rpx;
  background-color: #f5f5f5;
}
.searcher image {
  width: 26rpx;
  height: 26rpx;
  float: left;
  margin-left: 256rpx;
  margin-top: 24rpx;
}
.searcher input {
  width: 224rpx;
  height: 26rpx;
  float: left;
  margin-left: 10rpx;
  margin-top: 12rpx;
  font-size: 28rpx;
  color: #999;
}
/* 游学列表 */
.content {
  background-color: #f7f7f7;
  width: 100%;
}
.studyList {
  background-color: #fff;
  width: 100%;
  height: 288rpx;
}
.studyListChild {
  margin-top: 20rpx;
}
.studyList image {
  width: 216rpx;
  height: 216rpx;
  display: block;
  float: left;
  margin-top: 36rpx;
  margin-left: 30rpx;
}
.studyText {
  width: 420rpx;
  float: left;
  margin-left: 52rpx;
  margin-top: 26rpx;
}
.textTitle {
  font-size: 32rpx;
  color: #454545;
}
.textContent {
  /* 做省略限制 */
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 28rpx;
  color: #9e9e9e;
  height: 78rpx;
  margin-top: 20rpx;
}
.textMoney {
  display: block;
  color: #ff135b;
  font-size: 32rpx;
  margin-top: 30rpx;
  font-weight: 700;
  float: left;
}
.textDiscuss {
  display: block;
  color: #9e9e9e;
  font-size: 28rpx;
  float: left;
  margin-left: 178rpx;
  margin-top: 38rpx;
}
