<view class="head">
  <image src='../../images/jf_bj.png'></image>
  <!-- <text class="integral-exchange">积分兑换</text> -->
  <text class='Total'>总积分</text>
  <text class='integral'>{{data.score.score}}</text>
</view>
<view class='list'>
  <view class='item' wx:for="{{data.lists}}" wx:key='key'>
    <view class='top'>
      <view class='left'>
        <text class='name'>{{item.title}}</text>
        <text class='state'>{{item.status}}</text>
      </view>
      <view class='right'>
        <text>+{{item.score}}</text>
      </view>
    </view>
    <view class='time'>
      {{item.createtime}}
    </view>
  </view>

</view>