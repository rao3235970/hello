.list {
  padding: 0 30rpx;
}

.head {
  display: flex;
  justify-content: space-between;
  border-bottom: 1rpx solid rgba(238, 238, 238, 1);
  height: 74rpx;
  align-items: center;
}

.head text:nth-of-type(1) {
  font-size: 34rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(15, 29, 55, 1);
}

.head text:nth-of-type(2) {
  font-size: 34rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
}

.item {
  display: flex;
  /* height: 180rpx; */
  align-items: center;
  border-bottom: 1rpx solid rgba(229, 229, 229, 1);
  padding-top:15rpx;
  padding-bottom:15rpx;

}

.item .cont text:nth-of-type(1) {
  font-size: 30rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(15, 29, 55, 1);
  display: block;
}

.item .cont text:nth-of-type(2) {
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
}

.item-video {
  border-bottom: 1rpx solid rgba(229, 229, 229, 1);
  /* height: 486rpx; */

  display: flex;
  align-items: center;
  padding-bottom: 15rpx;
  padding-top: 15rpx;
}

.item-video .video {
  height: 360rpx;
  width: 100%;
  display: block;
}

.item-video text {
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(15, 29, 55, 1);
  /* height: 66rpx;
  line-height: 66rpx; */
  display: block;
  padding-top: 5px;
  /* border-bottom: 1rpx solid rgba(229, 229, 229, 1); */
}

.item-video .box {
  position: relative;
}

.item-video .box .zz {
  position: absolute;
  background: rgba(0, 0, 0, 0.5);
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.play {
  width: 52rpx;
  height: 60rpx;
  display: block;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -30rpx;
  margin-left: -26rpx;
}

.check {
  width: 52rpx;
  height: 48rpx;
  display: block;
  margin-right: 40rpx;
}

.footer {
  height: 88rpx;
  background: rgba(236, 236, 236, 1);
  position: fixed;
  width: 100%;
  bottom: 0;
  left: 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.footerbox {
  position: relative;
}

.footer-zw {
  height: 88rpx;
}

.footer image {
  width: 48rpx;
  height: 48rpx;
  display: block;
  margin-left: 30rpx;
}

.footer>view {
  display: flex;
  height: 100%;
  align-items: center;
}

.footer>view text {
  font-size: 34rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(15, 29, 55, 1);
  margin-left: 20rpx;
}

.footer>text {
  margin-right: 30rpx;
  font-size: 34rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
}

.isNull {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 200rpx;
  font-size: 50rpx;
  color: rgba(153, 153, 153, 1);
}
